// backend/prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model View {
  id        Int      @id @default(autoincrement())
  userId    String
  contentId String
  duration  Int
  timestamp DateTime @default(now())
}
